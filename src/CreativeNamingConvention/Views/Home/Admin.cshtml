@{
    ViewData["Title"] = "Contact";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="container creative-containter-top">
        <div class="row">
            <div class="col-sm-offset-2 col-sm-8">
                <div class="form-horizontal">
                    <div class="panel panel-primary">
                        <div class="panel-height panel-heading text-center">
                            <h3>Admin Dashboard for Adding & Editing Creative Options</h3>
                        </div>
                        <div class="panel-body">
                            <div class="form-group">  
                                <div class="col-md-offset-1 col-md-10">
                                    <!-- Row 1-->
                                    <div class="row">
                                        <div class="col-sm-4 form-group-sm">
                                            <a class="btn btn-info btn-block" href="/lobs" type="button">LOB</a>
                                        </div>
                                        <div class="col-sm-4 form-group-sm">
                                            <a class="btn btn-info btn-block" href="/products" type="button">Product</a>
                                        </div>
                                        <div class="col-sm-4 form-group-sm">
                                            <a class="btn btn-info btn-block" href="/creativecategories" type="button">Creative Category</a>
                                        </div>
                                    </div>
                                    <br />
                                    <!-- Row 2-->
                                    <div class="row">
                                        <div class="col-sm-4 form-group-sm">
                                            <a class="btn btn-info btn-block" href="/mediatypes" type="button">Media Type</a>
                                        </div>
                                        <div class="col-sm-4 form-group-sm">
                                            <a class="btn btn-info btn-block" href="/intendedscreens" type="button">Intended Screen</a>
                                        </div>
                                        <div class="col-sm-4 form-group-sm">
                                            <a class="btn btn-info btn-block" href="/creativetypes" type="button">Creative Type</a>
                                        </div>
                                    </div>
                                    <br />
                                    <!-- Row 3-->
                                    <div class="row">
                                        <div class="col-sm-4 form-group-sm">
                                            <a class="btn btn-info btn-block" href="/videolengths" type="button">Video Lengths</a>
                                        </div>
                                        <div class="col-sm-4 form-group-sm">
                                            <a class="btn btn-info btn-block" href="/abtestlabels" type="button">AB Test Labels</a>
                                        </div>
                                        <div class="col-sm-4 form-group-sm">
                                            <a class="btn btn-info btn-block" href="/creativeops" type="button">Creative Ops</a>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}


